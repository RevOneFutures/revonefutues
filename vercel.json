{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/templates",
      "dest": "/api/templates.ts"
    },
    {
      "src": "/api/preview/(.*)",
      "dest": "/api/preview/[templateId].ts"
    },
    {
      "src": "/preview.html",
      "dest": "/preview.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

